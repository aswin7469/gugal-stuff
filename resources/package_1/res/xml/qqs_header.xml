<?xml version='1.0' encoding='utf-8' ?>
<ConstraintSet android:id="@id/qqs_header_constraint" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
  <Constraint android:id="@id/clock">
    <Layout android:layout_width="wrap_content"
            android:layout_height="@dimen/large_screen_shade_header_min_height"
            app:layout_constraintBottom_toBottomOf="@id/qqs_header_bottom_guideline"
            app:layout_constraintEnd_toStartOf="@id/date"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="@id/begin_guide"
            app:layout_constraintTop_toTopOf="parent" />
    <Transform android:scaleX="1.0"
               android:scaleY="1.0" />
  </Constraint>
  <Constraint android:id="@id/date">
    <Layout android:layout_width="wrap_content"
            android:layout_height="@dimen/new_qs_header_non_clickable_element_height"
            android:layout_marginStart="8.0dp"
            app:layout_constrainedWidth="true"
            app:layout_constraintBaseline_toBaselineOf="@id/clock"
            app:layout_constraintEnd_toStartOf="@id/barrier"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@id/clock" />
  </Constraint>
  <Constraint android:id="@id/shade_header_system_icons">
    <Layout android:layout_width="wrap_content"
            android:layout_height="@dimen/new_qs_header_non_clickable_element_height"
            app:layout_constraintBottom_toBottomOf="@id/qqs_header_bottom_guideline"
            app:layout_constraintEnd_toEndOf="@id/end_guide"
            app:layout_constraintHeight_min="@dimen/new_qs_header_non_clickable_element_height"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toEndOf="@id/date"
            app:layout_constraintTop_toTopOf="parent" />
  </Constraint>
  <Constraint android:id="@id/carrier_group">
    <Layout android:layout_width="wrap_content"
            android:layout_height="@dimen/large_screen_shade_header_min_height"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintWidth_min="48.0dp" />
    <PropertySet android:alpha="0.0" />
  </Constraint>
  <Constraint android:id="@id/privacy_container">
    <Layout android:layout_width="wrap_content"
            android:layout_height="@dimen/large_screen_shade_header_min_height"
            app:layout_constraintBottom_toBottomOf="@id/qqs_header_bottom_guideline"
            app:layout_constraintEnd_toEndOf="@id/end_guide"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@id/date"
            app:layout_constraintTop_toTopOf="parent" />
  </Constraint>
</ConstraintSet>